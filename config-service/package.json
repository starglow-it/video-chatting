{
  "name": "config-service",
  "version": "0.2.0",
  "engines": {
    "node": ">=16.16.0"
  },
  "main": "src/index.ts",
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "node -r tsconfig-paths/register -r ts-node/register ./src/index.ts",
    "ext": "ts, js"
  },
  "scripts": {
    "build": "tsc --project tsconfig.server.json",
    "start": "node -r tsconfig-paths/register -r ts-node/register ./dist/config-service/src/index.js",
    "dev": "nodemon src/index.ts",
    "lint": "eslint ./src/",
    "lint:fix": "eslint ./src/ --fix",
    "lint:watch": "npm run lint --watch"
  },
  "dependencies": {
    "axios": "0.27.2",
    "body-parser": "1.20.0",
    "camelcase": "6.3.0",
    "compression": "1.7.4",
    "dotenv": "16.0.1",
    "express": "4.18.1",
    "http-status": "1.5.2",
    "method-override": "3.0.0",
    "morgan": "1.10.0",
    "tsconfig-paths": "4.0.0",
    "winston": "3.8.1",
    "winston-transport": "4.5.0"
  },
  "devDependencies": {
    "@types/amqplib": "0.8.2",
    "@types/compression": "1.7.2",
    "@types/express": "4.17.13",
    "@types/method-override": "0.0.32",
    "@types/morgan": "1.9.3",
    "@types/node": "18.0.5",
    "@typescript-eslint/eslint-plugin": "5.30.6",
    "@typescript-eslint/parser": "5.30.6",
    "eslint": "8.20.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-plugin-import": "2.26.0",
    "nodemon": "2.0.19",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  }
}
