{
  "name": "media-server-service",
  "version": "0.1.0",
  "engines": {
    "node": ">=16.16.0"
  },
  "main": "src/index.ts",
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "node -r tsconfig-paths/register -r ts-node/register ./src/index.ts",
    "ext": "ts, js"
  },
  "scripts": {
    "build": "tsc --project tsconfig.server.json",
    "start": "node -r tsconfig-paths/register -r ts-node/register ./dist/media-server-service/src/index.js",
    "dev": "nodemon src/index.ts",
    "lint": "eslint ./src/ --fix",
    "lint:fix": "eslint ./src/ --fix",
    "lint:watch": "npm run lint --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "package:update": "ncu -u"
  },
  "dependencies": {
    "amqplib": "0.10.3",
    "axios": "0.27.2",
    "dotenv": "16.0.2",
    "livekit-server-sdk": "1.0.2",
    "tsconfig-paths": "4.1.0",
    "npm-check": "6.0.1",
    "uuid": "9.0.0",
    "winston": "3.8.2"
  },
  "devDependencies": {
    "@types/amqplib": "0.8.2",
    "@types/express": "4.17.14",
    "@types/node": "18.7.18",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "eslint": "8.23.1",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-plugin-import": "2.26.0",
    "nodemon": "2.0.20",
    "prettier": "2.7.1",
    "ts-node": "10.9.1",
    "typescript": "4.8.3"
  }
}
